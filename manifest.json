{
    "name": "Admin tools",
    "description": "Useful tools for WebEOC",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*.192.168.100.100/*"],
            "js": ["js/script.js"]
        }
    ],
    "permissions": [
        "scripting",
        "webRequest",
        "activeTab"
    ],
    "icons": {
        "16":"./img/16x16.png",
        "32":"./img/32x32.png",
        "48":"./img/48x48.png",
        "128":"./img/128x128.png"
    }
}
